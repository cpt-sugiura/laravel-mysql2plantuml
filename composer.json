{
    "name": "molay76/laravel-mysql2plantuml",
    "license": "MIT",
    "description": "convert mysql schema to plantuml",
    "authors": [
        {
            "name": "cpt-sugiura",
            "email": "sugiura@cpoint-lab.co.jp"
        }
    ],
    "keywords": [
        "laravel",
        "PlantUML",
        "MySQL"
    ],
    "require": {
        "php": "^7.2",
        "laravel/framework": ">=6.20.12|>=7.30.3|>=8.22.1"
    },
    "autoload": {
        "psr-4": {
            "Mysql2PlantUml\\": "./src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mysql2PlantUml\\Mysql2PlantUmlServiceProvider"
            ]
        }
    }
}
